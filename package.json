    "docker:start:order": "docker run -d --name order-service-container -p 9006:9006 --env-file apps/order-service/.env order-service",
    "docker:stop:order": "docker stop order-service-container && docker rm order-service-container",
    "docker:build:payment": "docker build -t payment-service -f apps/payment-service/Dockerfile .",
    "docker:start:payment": "docker run -d --name payment-service-container -p 9007:9007 --env-file apps/payment-service/.env payment-service",
    "docker:stop:payment": "docker stop payment-service-container && docker rm payment-service-container",
    "docker:build:recommendation": "docker build -t recommendation-service -f apps/recommendation-service/Dockerfile .",
    "docker:start:recommendation": "docker run -d --name recommendation-service-container -p 9008:9008 --env-file apps/recommendation-service/.env recommendation-service",
    "docker:stop:recommendation": "docker stop recommendation-service-container && docker rm recommendation-service-container",
    "docker:build:review": "docker build -t review-service -f apps/review-service/Dockerfile .",
    "docker:start:review": "docker run -d --name review-service-container -p 9009:9009 --env-file apps/review-service/.env review-service",
    "docker:stop:review": "docker stop review-service-container && docker rm review-service-container"
